{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./",
    "paths": {
      "@src": ["src/*"],
      "@core": ["src/core"],
      "@infrastructure": ["src/infrastructure"],
      "@resolver": ["src/resolver"],
      "@use-case": ["src/use-case"]
    },
    "target": "es2022"
  },
  "minify": false
}
