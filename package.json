{
  "name": "music-streaming-services-playlist-history-backend",
  "version": "0.1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "tsnd -r tsconfig-paths/register --inspect=0.0.0.0:9229 --respawn src/main.ts",
    "test": "rimraf coverage .nyc_output && concurrently 'npm:test:unit' 'npm:test:e2e' && npm run calculate-global-test-coverage",
    "test:unit": "jest --config jest.config.unit.ts --coverageDirectory coverage/unit",
    "test:e2e": "jest --config jest.config.e2e.ts --coverageDirectory coverage/e2e",
    "calculate-global-test-coverage": "ts-node scripts/calculate-global-test-coverage.ts",
    "prepare": "husky",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "build:clean": "rimraf dist; exit 0",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "prismaStudio": "prisma studio"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@as-integrations/fastify": "^2.1.1",
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.15",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-fastify": "^10.4.15",
    "@prisma/client": "^5.21.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.9.0",
    "prisma": "^5.21.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.0.3",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.15",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@testcontainers/postgresql": "^10.16.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "nock": "^13.5.6",
    "nyc": "^15.1.0",
    "postgres": "^3.4.5",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "testcontainers": "^10.13.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
